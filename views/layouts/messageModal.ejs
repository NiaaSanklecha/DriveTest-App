<div
  class="modal fade"
  id="gradeModel"
  tabindex="-1"
  aria-labelledby="gradeModelLabel"
  aria-hidden="true"
>
  <div
    class="modal-dialog modal-fullscreen-md-down modal-dialog-centered modal-dialog-scrollable"
  >
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="gradeModelLabel">Message</h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>

      <div class="modal-body">
        <p><%= message %></p>
      </div>
    </div>
  </div>
</div>
